{
  "name": "write-house-api",
  "private": true,
  "version": "1.0.0",
  "description": "Write house backend repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "audit-ci": "audit-ci --config audit-ci.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm-cli": "ts-node ./node_modules/typeorm/cli.js $*",
    "migrate:run": "npm run typeorm-cli migration:run",
    "migrate:revert": "npm run typeorm-cli migration:revert",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -d src/database/migrations -n $*",
    "pm2:deploy:staging": "npm run build && pm2 start app.staging.json",
    "pm2:start:staging": "pm2 start app.staging.json",
    "pm2:stop:staging": "pm2 stop app.staging.json",
    "pm2:destroy:staging": "pm2 delete app.staging.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.186.0",
    "@aws-sdk/s3-request-presigner": "^3.186.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "2.0.1",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "8.0.4",
    "async": "^3.2.4",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.7.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "node-polyglot": "^2.4.2",
    "pg": "8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "0.2.34",
    "web3": "^1.7.5",
    "web3-utils": "^1.8.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/async": "^3.2.15",
    "@types/cache-manager": "^4.0.1",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "4.14.189",
    "@types/node": "^16.11.1",
    "@types/node-polyglot": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "audit-ci": "5.1.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.0",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
